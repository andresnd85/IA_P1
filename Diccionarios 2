teclado1 = {
	'Categoria': 'Teclados',
	'Modelo': 'HyperX Alloy FPS Pro',
	'Precio': '89,99'
}
#Estos diccionarios nos perniten almacenar diferente informacion de un mismo producto 
teclado2 = {
	'Categoria': 'Teclados',
	'Modelo': 'Corsair K55 RGB',
	'Precio': '59,99'
}
#El for nos imprime todos los datos 
#Con el .items obtenemos los datos del diccionario 
for x,y in teclado1.items():
	print(x, "=",y,".")
else:
